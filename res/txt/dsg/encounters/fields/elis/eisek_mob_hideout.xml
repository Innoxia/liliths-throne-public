.824s-.125 3.678.208 3.881M171.005 212.95c-2.858-2.126-1.733 4.062.056 1.104M174.729 212.679s.81 1.584 1.082 1.917M176.521 212.496s-1.75 2.891-1.604 3.745M178.584 212.031c-1.479-.145-1.771 2.7 0 2.674 1.771-.026 1.479-2.529 0-2.674zM180.752 212.026l.146 2.679s.727-5.484 1.935.286M185.481 210.866l.271 3.839M185.633 213.013c-3.089-1.584-1.67 3.771.119 1.062M188.502 214.158c-.669 1.646-2.58.021-1.958-.979 1.438-2.313 3.292.33 0 0M189.377 212.324v2.272M190.479 212.116s-.602.208-1.102.771M195.981 212.324c-2.063-1.146-2.542 3.604.125 1.917M198.273 212.116c1.458-.021 1.896 2.357.188 2.491-1.708.134-1.646-2.47-.188-2.491zM200.336 212.324v2.272M201.419 212.144s-.791.306-1.083.993M202.314 211.929l.188 2.854s.865-5.292 1.917 0M205.242 213.241c.862.154 3.366-.479 1.282-1.125-2.543-.788-.541 4.542.563 2.146M208.087 212.324v2.288M209.254 212.036s-.792.254-1.167.604M210.421 210.596s0 2.75-.167 3.021M210.254 214.262l-.072.334"/>
