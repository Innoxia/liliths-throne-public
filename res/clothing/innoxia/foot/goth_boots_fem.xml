<clothing>
	<coreAttributes>
		<value>1000</value> 
		
		<determiner><![CDATA[a pair of]]></determiner>
		
		<name><![CDATA[gothic boot]]></name>
		
		<namePlural pluralByDefault="true"><![CDATA[gothic boots]]></namePlural> 
		
		<description><![CDATA[A pair of feminine knee-high boots, made from high-quality leather. A series of straps and buckles, while having their functionality made redundant by means of a zip running up the inner sides, give these boots a very 'gothic' look.]]></description> 
		
		<physicalResistance>1</physicalResistance> 
		
		<femininity>FEMININE</femininity> 
		
		<slot>FOOT</slot> 
		
		<rarity>COMMON</rarity> 
		
		<clothingSet/> 
		
		<imageName>goth_boots_fem.svg</imageName> 
		
		<imageEquippedName/> 
		
		<effects/>
		
		<blockedPartsList> 
		
			<blockedParts>
				<displacementType>REMOVE_OR_EQUIP</displacementType> 
				
				<clothingAccessRequired> 
					<clothingAccess>FEET</clothingAccess>
				</clothingAccessRequired>
				
				<blockedBodyParts>
					<bodyPart>FEET</bodyPart>
				</blockedBodyParts>
				
				<clothingAccessBlocked> 
					<clothingAccess>FEET</clothingAccess>
				</clothingAccessBlocked>
				
				<concealedSlots>
					<slot>SOCK</slot>
				</concealedSlots>
			</blockedParts>
			
		</blockedPartsList>
		
		<incompatibleSlots/> 
		
		<primaryColours values="JUST_BLACK"/> 
		<primaryColoursDye values="ALL"/>
		<secondaryColours values="JUST_STEEL"/>
		<secondaryColoursDye values="ALL_METAL"/>
		<tertiaryColours values="JUST_BLACK"/>
		<tertiaryColoursDye values="ALL"/>
		
		<itemTags> 
			<tag>SOLD_BY_NYAN</tag>
		</itemTags>
	</coreAttributes>

  <replacementText type="REMOVE_OR_EQUIP">
    <self>
      <![CDATA[[npc.Name] [npc.verb(slip)] [npc.her] [npc.feet] into the boots, before zipping them up.]]>
    </self>
    <other>
      <![CDATA[[npc.Name] [npc.verb(slip)] [npc2.namePos] [npc2.feet] into the boots, before zipping them up.]]>
    </other>
    <otherRough>
      <![CDATA[[npc.Name] roughly [npc.verb(push)] [npc2.namePos] [npc2.feet] into the boots, before zipping them up.]]>
    </otherRough>
  </replacementText>

  <displacementText type="REMOVE_OR_EQUIP">
    <self>
      <![CDATA[[npc.Name] [npc.verb(unzip)] [npc.her] boots, before taking them off.]]>
    </self>
    <other>
      <![CDATA[[npc.Name] [npc.verb(unzip)] [npc2.namePos] boots, before taking them off.]]>
    </other>
    <otherRough>
      <![CDATA[[npc.Name] quickly [npc.verb(unzip)] [npc2.namePos] boots, before roughly tugging them off.]]>
    </otherRough>
  </displacementText>
	
