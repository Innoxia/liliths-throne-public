					<type>innoxia_dagger_dagger</type>
				</types>
				<damageTypes>
					<damage>FIRE</damage>
					<damage>ICE</damage>
					<damage>POISON</damage>
				</damageTypes>
				<primaryColours/>
				<secondaryColours/>
			</weapon>
			<weapon>
				<conditional><![CDATA[!condExhibitionist]]></conditional>
				<type>innoxia_crystal_epic</type>
				<damageTypes>
					<damage>FIRE</damage>
					<damage>ICE</damage>
					<damage>POISON</damage>
				</damageTypes>
				<primaryColours/>
				<secondaryColours/>
			</weapon>
			<weapon>
				<conditional><![CDATA[condExhibitionist]]></conditional>
				<type>innoxia_bdsm_riding_crop</type>
				<damageTypes>
					<damage>LUST</damage>
				</damageTypes>
				<primaryColours/>
				<secondaryColours/>
			</weapon>
        </mainWeapons>
		
        <offhandWeapons>
			<weapon>
				<conditional/>
				<types>
					<type>innoxia_feather_epic</type>
				</types>
				<damageTypes>
